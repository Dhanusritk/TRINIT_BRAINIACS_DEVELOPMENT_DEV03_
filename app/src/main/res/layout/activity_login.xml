<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginVM" type="com.dhanusrisapplication.app.modules.login.data.viewmodel.LoginVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearLogin"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageWeeding"
            android:layout_height="@dimen/_45pxh"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="@dimen/_51pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_weeding"
            tools:ignore="ContentDescription" tools:src="@drawable/img_weeding"/>
        <TextView android:ellipsize="end" android:id="@+id/txtSignIn"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_34pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.01" android:singleLine="true"
            android:text="@{loginVM.loginModel.txtSignIn}"
            android:textAllCaps="true"
            style="@style/txtRubikromanmedium20" tools:text="@string/lbl_sign_in"/>
        <TextView android:id="@+id/txtConfirmation"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_4pxh"
            android:text="@{loginVM.loginModel.txtConfirmation}"
            style="@style/txtRubikromanregular14" tools:text="@string/msg_looks_like_you"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_radius_8"
            android:ellipsize="end" android:hint="@string/lbl_email"
            android:id="@+id/etInputinitial"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent"
            android:letterSpacing="0.03"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@={loginVM.loginModel.etInputinitialValue}"
            android:textColorHint="@color/bluegray_700"
            style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_radius_8"
            android:ellipsize="end" android:hint="@string/lbl_password"
            android:id="@+id/etInputinitialOne"
            android:inputType="textPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:letterSpacing="0.03"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_15pxv" android:singleLine="true"
            android:text="@={loginVM.loginModel.etInputinitialOneValue}"
            android:textColorHint="@color/bluegray_700"
            style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnLogin"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:letterSpacing="0.01"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_17pxv" android:singleLine="true"
            android:text="@string/lbl_login" android:textAllCaps="true"
            style="@style/btnSolidRounded" tools:text="@string/lbl_login"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowlinefour"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <View android:background="@color/gray_200"
                android:id="@+id/lineLineFour"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.28" android:layout_width="0dp"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_weight="0.45" android:layout_width="0dp"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{loginVM.loginModel.txtLanguage}"
                android:textAllCaps="true"
                style="@style/txtRubikromanregular14_1" tools:text="@string/msg_forgot_password"/>
            <View android:background="@color/gray_200"
                android:id="@+id/lineLineThree"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.28" android:layout_width="0dp"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtLanguageOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.01" android:singleLine="true"
            android:text="@{loginVM.loginModel.txtLanguageOne}"
            android:textAllCaps="true"
            style="@style/txtRubikromanregular14_2" tools:text="@string/lbl_or_signup"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowgoogle"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegray_100cornerRadius">
            <ImageView android:id="@+id/imageGoogle"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="@dimen/_15pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_google"
                tools:ignore="ContentDescription" tools:src="@drawable/img_google"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignInwithGo"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_93pxh"
                android:layout_marginStart="@dimen/_99pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_weight="0.89" android:layout_width="0dp"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{loginVM.loginModel.txtSignInwithGo}"
                style="@style/txtRubikromanregular14_3" tools:text="@string/msg_sign_in_with_go"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowfire"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegray_100cornerRadius">
            <ImageView android:id="@+id/imageFire"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_fire"
                tools:ignore="ContentDescription" tools:src="@drawable/img_fire"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignInwithAp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_103pxh"
                android:layout_marginStart="@dimen/_98pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_weight="0.88" android:layout_width="0dp"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{loginVM.loginModel.txtSignInwithAp}"
                style="@style/txtRubikromanregular14_3" tools:text="@string/msg_sign_in_with_ap"/>
        </LinearLayout>
        <View android:background="@drawable/rectangle_radius_12"
            android:id="@+id/viewStylelayer"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_48pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_358pxh"/>
    </LinearLayout>
</layout>
