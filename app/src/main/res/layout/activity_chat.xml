<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="chatVM" type="com.dhanusrisapplication.app.modules.chat.data.viewmodel.ChatVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackcut"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <FrameLayout android:id="@+id/frameChat"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStyle">
            <LinearLayout android:id="@+id/linearColumncut"
                android:layout_gravity="top|center"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearIOSStatus"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageCut"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_width="@dimen/_54pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_cut"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_cut"/>
                        <ImageView android:id="@+id/imageRightSide"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_233pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="@dimen/_66pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rightside"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rightside"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_33pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_blue_51_radius_15_6"
                        android:id="@+id/btnArrowleft"
                        android:layout_height="@dimen/_31pxh"
                        android:layout_width="@dimen/_31pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBack"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_weight="0.18"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{chatVM.chatModel.txtBack}"
                        style="@style/txtInterbold16" tools:text="@string/lbl_back"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCommunityForum"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.67"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{chatVM.chatModel.txtCommunityForum}"
                        style="@style/txtInterbold16_1" tools:text="@string/lbl_community_forum2"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{chatVM.chatModel.txtTime}"
                    style="@style/txtIntermedium12" tools:text="@string/lbl_08_15"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowprofile50px"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameProfileUserAc"
                        android:layout_height="@dimen/_50pxv"
                        android:layout_marginBottom="@dimen/_26pxv" android:layout_width="@dimen/_50pxh">
                        <ImageView android:id="@+id/imageProfile50px"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_50pxh"
                            android:layout_width="@dimen/_50pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_profile50px}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_profile50px"/>
                        <View
                            android:background="@drawable/rectangle_bg_teal_400_border_white_a700_radius_6"
                            android:id="@+id/viewStatus"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_12pxh"/>
                    </FrameLayout>
                    <FrameLayout android:id="@+id/frameStackrectangle"
                        android:layout_height="@dimen/_76pxv"
                        android:layout_marginStart="@dimen/_11pxh" android:layout_width="@dimen/_280pxh">
                        <View
                            android:background="@drawable/rectangle_bg_gray_50_radius_8"
                            android:id="@+id/viewRectangle"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_60pxv"
                            android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_280pxh"/>
                        <TextView android:gravity="top|start"
                            android:id="@+id/txtDescription"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_width="match_parent"
                            android:text="@{chatVM.chatModel.txtDescription}"
                            style="@style/txtIntermedium16_1" tools:text="@string/msg_my_aloe_vera_is2"/>
                    </FrameLayout>
                </LinearLayout>
                <EditText
                    android:background="@drawable/rectangle_bg_green_900_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/msg_send_me_some_pi"
                    android:id="@+id/etGroupFive"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginStart="@dimen/_143pxh"
                    android:layout_marginTop="@dimen/_51pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_10pxh"
                    android:paddingTop="@dimen/_16pxv"
                    android:singleLine="true"
                    android:text="@={chatVM.chatModel.etGroupFiveValue}"
                    android:textColorHint="@color/white_A700"
                    style="@style/etSolidRounded_3" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTimeOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{chatVM.chatModel.txtTimeOne}"
                    style="@style/txtIntermedium12" tools:text="@string/lbl_11_40"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowsadmandpmin"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageSadManDPmin"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_width="@dimen/_49pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_sadmandpmin}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_sadmandpmin"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_50_radius_8"
                        android:ellipsize="end"
                        android:hint="@string/msg_my_plant_has_so"
                        android:id="@+id/etGroupThree"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_weight="0.81"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_3pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_22pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@={chatVM.chatModel.etGroupThreeValue}"
                        android:textColorHint="@color/bluegray_900"
                        style="@style/etSolidRounded_4" tools:ignore="Autofill"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowrectangleOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_19pxh"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangleOne"
                        android:layout_height="@dimen/_129pxh"
                        android:layout_width="@dimen/_130pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_8pxh}"
                        imageRes="@{@drawable/img_rectangle}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
                    <ImageView android:id="@+id/imageRectangleTwo"
                        android:layout_height="@dimen/_129pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_width="@dimen/_130pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_8pxh}"
                        imageRes="@{@drawable/img_rectangle_129x130}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_129x130"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTimeTwo"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{chatVM.chatModel.txtTimeTwo}"
                    style="@style/txtIntermedium12" tools:text="@string/lbl_11_43"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumndoyouknowwha"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="wrap_content"
                    android:orientation="vertical" style="@style/groupStylegreen_900cornerRadius">
                    <TextView android:gravity="start"
                        android:id="@+id/txtDoyouknowwha"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:text="@{chatVM.chatModel.txtDoyouknowwha}"
                        style="@style/txtIntermedium16_2" tools:text="@string/msg_it_i"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowdoyouknowwhaOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageImagesEleven"
                        android:layout_height="@dimen/_49pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="@dimen/_51pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_images11}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_images11"/>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumndoyouknowwhaOne"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylegray_50cornerRadius">
                        <TextView android:gravity="start"
                            android:id="@+id/txtDoyouknowwhaOne"
                            android:layout_height="match_parent"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:text="@{chatVM.chatModel.txtDoyouknowwhaOne}"
                            style="@style/txtIntermedium16_1" tools:text="@string/msg_can_anybody_sug"/>
                    </LinearLayout>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{chatVM.chatModel.txtLanguage}"
                    style="@style/txtIntermedium16_3" tools:text="@string/msg_reply"/>
                <EditText
                    android:background="@drawable/rectangle_gradient_s_white_a700_e_white_a700_00_border_bluegray_200_radius_8"
                    android:drawableEnd="@drawable/img_send"
                    android:drawablePadding="@dimen/_30pxh"
                    android:ellipsize="end"
                    android:hint="@string/lbl_message"
                    android:id="@+id/etInputMessageE"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_19pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_21pxv"
                    android:singleLine="true"
                    android:text="@={chatVM.chatModel.etInputMessageEValue}"
                    android:textColorHint="@color/bluegray_200"
                    style="@style/etGradientRoundedOutline" tools:ignore="Autofill"/>
            </LinearLayout>
        </FrameLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/teal_A400"
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_36pxh"
            android:layout_marginStart="@dimen/_36pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_60pxh"
            app:srcCompat="@drawable/img_buttonfloat60" app:tint="@color/black_900"/>
    </FrameLayout>
</layout>
