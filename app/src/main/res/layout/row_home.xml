<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeRowModel" type="com.dhanusrisapplication.app.modules.home.data.model.HomeRowModel"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearCard"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylecornerRadius_1">
        <LinearLayout android:id="@+id/linearRowduration"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearRowellipse192"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_weight="0.67" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEllipse192"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse192}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse192"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtThanhPham"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="0.75"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{homeRowModel.txtThanhPham}"
                    style="@style/txtRubikitaliclight16" tools:text="@string/lbl_thanh_pham"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDuration"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_111pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_weight="0.33" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{homeRowModel.txtDuration}"
                style="@style/txtRubikitaliclight14" tools:text="@string/lbl_1_hour_ago"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageMsnyz9L6gsFour"
            android:layout_height="@dimen/_224pxh"
            android:layout_width="@dimen/_335pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_msnyz9l6gs4}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_msnyz9l6gs4"/>
        <LinearLayout android:id="@+id/linearRowpluscircleone"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_A700">
            <ImageView android:id="@+id/imagePluscircleOne"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_pluscircle1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_pluscircle1"/>
            <LinearLayout android:id="@+id/linearRowtwenty"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_182pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.87" android:layout_width="0dp" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTwenty"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.24"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{homeRowModel.txtTwenty}"
                    style="@style/txtRubikitaliclight14_1" tools:text="@string/lbl_20"/>
                <ImageView android:id="@+id/imageChat"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_chat"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_chat"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOneHundredTwentyFive"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.31"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{homeRowModel.txtOneHundredTwentyFive}"
                    style="@style/txtRubikitaliclight14_1" tools:text="@string/lbl_125"/>
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_14x16"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_14x16"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
