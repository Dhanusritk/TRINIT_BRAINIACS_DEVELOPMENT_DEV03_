<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frame1590VM" type="com.dhanusrisapplication.app.modules.frame1590.data.viewmodel.Frame1590VM"/>
    </data>
    <FrameLayout android:id="@+id/frameStacklanguage"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnlanguage"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_2pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end" android:gravity="end"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_46pxh"
                android:layout_marginStart="@dimen/_46pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{frame1590VM.frame1590Model.txtLanguage}"
                style="@style/txtIntermedium24" tools:text="@string/msg_smart_remainder"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowlanguageOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_43pxh"
                android:layout_marginStart="@dimen/_43pxh"
                android:layout_marginTop="@dimen/_91pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStack"
                    android:layout_height="@dimen/_193pxv"
                    android:layout_marginBottom="@dimen/_1pxv" android:layout_width="@dimen/_163pxh">
                    <FrameLayout android:id="@+id/frameStacklanguageOne"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_193pxv"
                        android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_153pxh">
                        <TextView android:gravity="top|start"
                            android:id="@+id/txtLanguageOne"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:text="@{frame1590VM.frame1590Model.txtLanguageOne}"
                            style="@style/txtIntermedium24" tools:text="@string/msg_today_s_tasks"/>
                        <ImageView android:id="@+id/imageImageFifteen"
                            android:layout_gravity="bottom|start"
                            android:layout_height="@dimen/_44pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_9pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_43pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_image15"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image15"/>
                    </FrameLayout>
                    <ImageView android:id="@+id/imageImageFourteen"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_52pxh"
                        android:layout_marginBottom="@dimen/_63pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_63pxv"
                        android:layout_width="@dimen/_51pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_image14}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image14"/>
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumn"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_69pxh"
                    android:layout_marginTop="@dimen/_53pxv"
                    android:layout_weight="0.24"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageImageSixteen"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_image16"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image16"/>
                    <ImageView android:gravity="center_horizontal"
                        android:id="@+id/imageImageSeventeen"
                        android:layout_height="@dimen/_48pxh"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_50pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_image17"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image17"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearColumnlanguageTwo"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleteal_300_bfcornerRadius">
                <FrameLayout android:id="@+id/frameStacklanguageTwo"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_110pxv"
                    android:layout_marginEnd="@dimen/_46pxh"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_69pxv" android:layout_width="@dimen/_280pxh">
                    <ImageView android:id="@+id/imageImageEighteen"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_68pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_76pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_image18}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image18"/>
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtLanguageTwo"
                        android:layout_gravity="top|end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:text="@{frame1590VM.frame1590Model.txtLanguageTwo}"
                        style="@style/txtIntermedium24" tools:text="@string/msg_upcoming_tasks"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFebCounter"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_104pxv"
                    android:layout_marginEnd="@dimen/_149pxh"
                    android:layout_marginStart="@dimen/_149pxh"
                    android:layout_marginTop="@dimen/_42pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{frame1590VM.frame1590Model.txtFebCounter}"
                    style="@style/txtIntermedium14_2" tools:text="@string/lbl_feb_27"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageThree"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_43pxh"
                android:layout_marginStart="@dimen/_43pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{frame1590VM.frame1590Model.txtLanguageThree}"
                style="@style/txtIntermedium24_1" tools:text="@string/lbl_back2"/>
        </LinearLayout>
    </FrameLayout>
</layout>
